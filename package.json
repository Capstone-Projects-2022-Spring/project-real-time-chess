{
    "name": "real-time-chess",
    "version": "0.0.1",
    "description": "A realtime chess application",
    "main": "dist/index.js",
    "module": "commonjs",
    "root": "./dist",
    "scripts": {
        "build": "npm run build-server & npm run build-sass & webpack",
        "build-server": "tsc",
        "build-sass": "sass static/src/scss:static/public/css",
        "start": "node dist/main/index.js",
        "clean": "rm -r dist",
        "coverage": "nyc --reporter=lcov --reporter=text-lcov mocha --timeout 10000 -r ts-node-test-register -r tsconfig-paths/register \"src/test/**/*.spec.ts\" --exit",
        "test": "mocha --reporter spec -r ts-node-test-register -r tsconfig-paths/register \"src/test/**/*.spec.ts\" --exit",
        "gen-uml": "tsuml2 --glob static/src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Capstone-Projects-2022-Spring/project-real-time-chess.git"
    },
    "author": "Viraj Shah",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Capstone-Projects-2022-Spring/project-real-time-chess/issues"
    },
    "homepage": "https://github.com/Capstone-Projects-2022-Spring/project-real-time-chess#readme",
    "dependencies": {
        "assert": "^2.0.0",
        "axios": "^0.25.0",
        "body-parser": "^1.19.1",
        "chess.js": "^0.12.1",
        "cookie-parser": "^1.4.6",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "express": "^4.17.2",
        "mongodb": "^4.4.0",
        "pug": "^3.0.2",
        "react": "^17.0.2",
        "react-chessboard": "^1.2.2",
        "react-dom": "^17.0.2",
        "socket.io": "^4.4.1",
        "sweetalert2": "^11.4.4",
        "winston": "^3.6.0"
    },
    "devDependencies": {
        "@types/assert": "^1.5.6",
        "@types/axios": "^0.14.0",
        "@types/body-parser": "^1.19.2",
        "@types/chess.js": "^0.11.2",
        "@types/chessboardjs": "^0.3.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.21",
        "@types/pug": "^2.0.6",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "eslint": "^8.11.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "mocha": "^9.2.1",
        "nyc": "^15.1.0",
        "prettier": "^2.5.1",
        "ts-loader": "^9.2.6",
        "ts-node-test-register": "^10.0.0",
        "tsconfig-paths": "^3.12.0",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2"
    }
}
